{
  "schemaVersion": 1,
  "id": "hotbarsplus",
  "version": "${version}",
  "name": "Hotbars+",
  "description": "Get more use out of your saved hotbars!",
  "authors": ["videogamesm12"],
  "contact": {
    "issues": "https://github.com/VideoGameSmash12/HotbarsPlus/issues",
    "homepage": "https://github.com/VideoGameSmash12/HotbarsPlus",
    "sources": "https://github.com/VideoGameSmash12/HotbarsPlus"
  },
  "license": "MIT",
  "icon": "assets/hotbarsplus/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.videogamesm12.hotbarsplus.core.HBPCore"
    ],
    "hotbarsplus": [
      "me.videogamesm12.hotbarsplus.core.provider.NotificationRouteProvider"
    ]
  },
  "mixins": [
    "hotbarsplus.mixin.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=1.20.3"
  },
  "accessWidener": "hotbarsplus.accesswidener",
  "breaks": {
    "multihotbar": "*"
  }
}
