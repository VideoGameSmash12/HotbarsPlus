{
  "schemaVersion": 1,
  "id": "hotbarsplus",
  "version": "${version}",
  "name": "Hotbars+",
  "description": "Get more use out of your saved hotbars!",
  "authors": ["videogamesm12"],
  "contact": {
    "issues": "https://github.com/VideoGameSmash12/HotbarsPlus/issues",
    "homepage": "https://github.com/VideoGameSmash12/HotbarsPlus",
    "sources": "https://github.com/VideoGameSmash12/HotbarsPlus"
  },
  "license": "MIT",
  "icon": "assets/hotbarsplus/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.videogamesm12.hotbarsplus.client.HotbarsPlusClient"
    ],
    "main": [
      "me.videogamesm12.hotbarsplus.HotbarsPlus"
    ],
    "modmenu":[
      "me.videogamesm12.hotbarsplus.integration.ModMenuIntegration"
    ]
  },
  "mixins": [
    "hotbarsplus.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.12.11",
    "fabric": ">=0.44.0+1.17",
    "minecraft": ">=1.17.1",
    "cloth-config2": ">=5.1.40"
  },
  "suggests": {
    "modmenu": ">=2.0.14"
  },
  "breaks": {
    "multihotbar": "*"
